      <keyCalculation datatype="TINYINT" expressionLanguage="SQL">
        <formula>CASE 
    WHEN &quot;REPORTING_DATE&quot; &gt;= ADD_DAYS(now(),-8) THEN 1
    ELSE 0
END
</formula>
      </keyCalculation>
